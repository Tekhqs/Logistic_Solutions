                                                                            .Name("SOAccount")
                                                                            .DataTextField("Status")
                                                                            .DataValueField("OrderId")
                                                                            .HtmlAttributes(new { style = "width:100%" })
                                                                            .Filter("contains")
                                                                            .OptionLabel("Select")
                                                                            .DataSource(source =>
                                                                            {
                                                                                source.Read(read =>
                                                                                {
                                                                                    read.Action("UserData", "Home");
                                                                                })
                                                                                           .ServerFiltering(true);
                                                                            })
                                                                            .HtmlAttributes(new { @required = "required", @validationMessage = "Account is required" })
                            )
                        </div>

                        <div class="SingleInput">
                            <label for="SOShipper">Shipper</label>
                            @(Html.Kendo().DropDownList()
                                                                                 .Name("SOShipper")
                                                                                 .DataTextField("Status")
                                                                                 .DataValueField("OrderId")
                                                                                 .HtmlAttributes(new { style = "width:100%" })
                                                                                 .Filter("contains")
                                                                                 .OptionLabel("Select")
                                                                                 .DataSource(source =>
                                                                                 {
                                                                                     source.Read(read =>
                                                                                     {
                                                                                         read.Action("UserData", "Home");
                                                                                     })
                                                                                                .ServerFiltering(true);
                                                                                 })
                            )
                        </div>
                        <div class="SingleInput">
                            <label for="SOCarrier">Carrier</label>
                            @(Html.Kendo().DropDownList()
                               .Name("SOCarrier")
                          .DataValueField("OrderId")
                         .HtmlAttributes(new { style = "width:100%" })
                          .Filter("contains")
                         .OptionLabel("Select")
                         .DataSource(source =>
                {
                           source.Read(read =>
                            {
                         read.Action("UserData", "Home");
                         })
                         .ServerFiltering(true);
                        })
                          .HeaderTemplate("<div onclick=\"ClickForModalCareer()\" style=\"cursor:pointer\" class=\"dropdown-header k-widget k-header \">" +
                              " <a  style=\"font-size:16px; text-decoration:none;\" class=\"linkColor\" > <i  style=\"margin-right:15px\" class=\"fas fa-plus\"></i> Add New</a>" +
                            "</div>")
                            )

                        </div>




                    </div>
                    <div class="col-sm-6 col-xs-12 form-group">



                        <div class="SingleInput">
                            <label for="SOBillFT">Bill Freight To</label>
                         @(Html.Kendo().DropDownList()
.Name("SOBillFT")
.DataTextField("Status")
.DataValueField("OrderId")
.HtmlAttributes(new { style = "width:100%" })
.Filter("contains")
.OptionLabel("Select")
.DataSource(source =>
{
source.Read(read =>
{
read.Action("UserData", "Home");
})
.ServerFiltering(true);
})
)
                        </div>
                        <div class="SingleInput">
                            <label for="SOShipTo">Ship To</label>

                            @(Html.Kendo().DropDownList()
                                                          .Name("SOShipTo")
                                                          .DataTextField("Status")
                                                          .DataValueField("OrderId")
                                                          .HtmlAttributes(new { style = "width:100%" })
                                                          .Filter("contains")
                                                          .OptionLabel("Select")
                                                          .DataSource(source =>
                                                          {
                                                              source.Read(read =>
                                                              {
                                                                  read.Action("UserData", "Home");
                                                              })
                                                              .ServerFiltering(true);
                                                          })
                                                           .HeaderTemplate("<div onclick=\"ClickForModal()\" style=\"cursor:pointer\" class=\"dropdown-header k-widget k-header \">" +
                               " <a  style=\"font-size:16px; text-decoration:none;\" class=\"linkColor\" > <i  style=\"margin-right:15px\" class=\"fas fa-plus\"></i> Add New</a>" +
                               "</div>")
                            )

                        </div>
                        <div class="SingleInput">
                            <label for="SOMarkFor">Mark For</label>
                            @(Html.Kendo().DropDownList()
                                                                                          .Name("SOMarkFor")
                                                                                          .DataTextField("Status")
                                                                                          .DataValueField("OrderId")
                                                                                          .HtmlAttributes(new { style = "width:100%" })
                                                                                          .Filter("contains")
                                                                                          .OptionLabel("Select")
                                                                                          .DataSource(source =>
                                                                                          {
                                                                                              source.Read(read =>
                                                                                              {
                                                                                                  read.Action("UserData", "Home");
                                                                                              })
                                                                                              .ServerFiltering(true);
                                                                                          })
                                                                                           .HeaderTemplate("<div onclick=\"ClickForModal()\" style=\"cursor:pointer\" class=\"dropdown-header k-widget k-header \">" +
                                                               " <a  style=\"font-size:16px; text-decoration:none;\" class=\"linkColor\" > <i  style=\"margin-right:15px\" class=\"fas fa-plus\"></i> Add New</a>" +
                                                               "</div>")
                            )
                        </div>





                    </div>

                </div>



            </div>





            <div class="col-xs-12 CreateUserSection pl-5">
                <h4 class="text-left">Dates</h4>
                <div class="row CreateUserform">
