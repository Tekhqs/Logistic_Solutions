                    </div>
                    <div class="col-sm-6 col-xs-12 form-group">
                        <div class="SingleInput">
                            <label for="SOCancelDate">Cancel  Date</label>
                            @(Html.Kendo().TextBox().Name("SOCancelDate").HtmlAttributes(new { @class = "form-control datepicker", @placeholder = "Enter  Cancel Date", @style = "width:100%", @type = "date" }))

                        </div>
                        <div class="SingleInput">
                            <label for="SOCancelDate">Deliver By  Date</label>
                            @(Html.Kendo().TextBox().Name("SODeliverBy").HtmlAttributes(new { @class = "form-control datepicker", @placeholder = "Enter  Deliver by Date", @style = "width:100%", @type = "date" }))

                        </div>

                    </div>


                </div>



            </div>

            <div class="col-xs-12 CreateUserSection pl-5">
                <h4 class="text-left">Other Instuctions</h4>
                <div class="row CreateUserform">
                    <div class="col-sm-12 col-xs-12 form-group">
                        <div class="SingleInput">
                            @Html.TextArea("SoInstructions", "", new { @class = "form-control ", @placeholder = "Add Instuctions", style = "width: 100%;" })

                        </div>

                    </div>


                </div>



            </div>



            <div class="col-xs-12 CreateUserSection pl-5">


                <div class="row">
                    <div class="col-xs-12">
                        <button class="Button">Cancel</button>
                        <button id="CreateSO" onclick="CreateShippingO()" class="Button">Create</button>
                    </div>
                </div>

            </div>

        </form>

    </div>


</div>

