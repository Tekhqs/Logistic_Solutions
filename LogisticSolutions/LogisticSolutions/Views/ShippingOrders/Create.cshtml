        <form id="FormShippingOrder">
            <div class="col-xs-12 CreateUserSection pl-5">
                <h4 class="text-left">Create Shipping Order</h4>
                <div class="row CreateUserform">
                    <div class="col-sm-6 col-xs-12 form-group">

                        <div class="SingleInput">
                            <label for="SOStatus">Status</label>
                            <select id="SOStatus" class="form-control">
                                <option> Open </option>
                                <option> Inprocess </option>
                                <option> Hold </option>
                                <option> Completed </option>
                                <option> Void </option>
                            </select>
                        </div>


                        <div class="SingleInput">
                            <label for="SOrderNumber">Order Number</label>
 @(Html.Kendo().TextBox().Name("SOrderNumber").HtmlAttributes(new { @class = "form-control", @placeholder = "Enter Order Number", @style = "width:100%", @type = "text" }))

                        </div>
                        <div class="SingleInput">
                            <label for="SOSWC">SWC</label>
                   @(Html.Kendo().TextBox().Name("SOSWC").HtmlAttributes(new { @class = "form-control", @placeholder = "Enter SWC", @style = "width:100%", @type = "text" }))

                        </div>

                        <div class="SingleInput">
                            <label for="SOSalesONumber">Sales Order Number</label>
                       @(Html.Kendo().TextBox().Name("SOSalesONumber").HtmlAttributes(new { @class = "form-control", @placeholder = "Enter Sales Order Number", @style = "width:100%", @type = "text" }))

                        </div>
                        <div class="SingleInput">
                            <label for="SOPONumber">PO Number</label>
                            @(Html.Kendo().TextBox().Name("SOPONumber").HtmlAttributes(new { @class = "form-control", @placeholder = "Enter PO Number", @style = "width:100%", @type = "number" }))

                        </div>
                        <div class="SingleInput">
                            <label for="SOBlNumber">BL Number</label>
                            @(Html.Kendo().TextBox().Name("SOBlNumber").HtmlAttributes(new { @class = "form-control", @placeholder = "Enter BL Number", @style = "width:100%", @type = "text" }))

                        </div>

                    </div>
                    <div class="col-sm-6 col-xs-12 form-group">


                        <div class="SingleInput">
                            <label for="SOBillFNo">Bill Freight Terms Number</label>
            @(Html.Kendo().TextBox().Name("SOBillFNo").HtmlAttributes(new { @class = "form-control", @placeholder = "Enter Bill Freight Terms Number", @style = "width:100%", @type = "text" }))

                        </div>
                        <div class="SingleInput">
                            <label for="SOQtyP">Qty Planned</label>
                            <select id="SOQtyP" class="form-control">
                                <option> 0-0EA </option>
                                <option> 0 CS </option>
                                <option> 0 IP </option>

                            </select>
                        </div>
                        <div class="SingleInput">
                            <label for="SOQtyS">Qty Shipped</label>
                            <select id="SOQtyS" class="form-control">
                                <option> 0-0EA </option>
                                <option> 0 CS </option>
                                <option> 0 IP </option>

                            </select>
                        </div>
